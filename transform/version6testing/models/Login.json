{
  "type": "object",
  "comment": "Login",
  "properties": {
    "username": {
      "type": "string",
      "label": "username",
      "control": "text",
      "selector": "input[name='username']"
    },
    "password": {
      "type": "string",
      "label": "password",
      "control": "text",
      "selector": "input[name='password']"
    },
    "submit": {
      "type": "action",
      "control": "action.click",
      "selector": "input[class='button']"
    },
    "islogin": {
      "type": "identifier",
      "control": "exists",
      "selector": "form[action='login.php']"
    }
  },
  "id": "Login",
  "connect": [
    {
      "match": [
        "islogin"
      ],
      "event": "submit"
    }
  ]
}
